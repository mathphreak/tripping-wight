package assets
{
	import tonfall.format.wav.WAVDecoder;
	/**
	 * @author Andre Michelle
	 */
	public final class Loop
	{
		[ Embed( source='../../assets/loops/amenbreak.wav', mimeType='application/octet-stream' ) ]
			private static const AMEN_BREAK_CLASS: Class;
			
		public static const AMEN_BREAK: WAVDecoder = new WAVDecoder( new AMEN_BREAK_CLASS() );
		
		[ Embed( source='../../assets/loops/instantcoffee.wav', mimeType='application/octet-stream' ) ]
			private static const INSTANT_COFFEE_CLASS: Class;
			
		public static const INSTANT_COFFEE: WAVDecoder = new WAVDecoder( new INSTANT_COFFEE_CLASS() );
	}
}